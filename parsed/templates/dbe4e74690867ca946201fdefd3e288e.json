{
  "pages/custom/custom-footer-desktop-container": "<div class=\"desktop-footer\">\r\n    {{#if theme_settings.show_copyright_footer}}\r\n    <div class=\"desktop-footer-copyright\">\r\n        <p class=\"powered-by\">&copy; {{moment format=\"YYYY\"}} {{settings.store_name}}. All rights reserved.</p>\r\n    </div>\r\n    {{/if}}\r\n    <div class=\"desktop-icon-container\">\r\n        <div class=\"inner-container\">\r\n            {{#each pages}}\r\n            {{#if name '===' \"Facebook \"}}\r\n            <a href=\"{{url}}\" aria-label=\"{{name}}\" target=\"_blank\">\r\n                <img src=\"{{cdn 'assets/img/custom-img/facebook-logo.png'}}\" />\r\n            </a>\r\n            {{/if}}\r\n            {{#if name '===' \"Twitter\"}}\r\n            <a href=\"{{url}}\" aria-label=\"{{name}}\" target=\"_blank\">\r\n                <img src=\"{{cdn 'assets/img/custom-img/tweeter-logo.png'}}\" />\r\n            </a>\r\n            {{/if}}\r\n            {{#if name '===' \"Youtube\"}}\r\n            <a href=\"{{url}}\" aria-label=\"{{name}}\" target=\"_blank\">\r\n                <img src=\"{{cdn 'assets/img/custom-img/youtube-logo.png'}}\" />\r\n            </a>\r\n            {{/if}}\r\n            {{/each}}\r\n        </div>\r\n    </div>\r\n</div>\r\n"
}
