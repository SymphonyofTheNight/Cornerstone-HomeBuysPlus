{
  "components/common/header": "<a href=\"#main-content\" class=\"skip-to-main-link\">{{lang 'header.skip_to_main'}}</a>\n{{#if banners.top}}\n<div class=\"banners\" data-banner-location=\"top\">\n    {{#each (limit banners.top_metadata 1)}}\n    <div class=\"banner\" data-event-type=\"promotion\" data-entity-id=\"{{this.id}}\" data-name=\"{{this.banner-name}}\"\n        data-position=\"{{this.location}}\" data-banner-id=\"{{this.banner-name}}\">\n        <div data-event-type=\"promotion-click\">\n            {{{this.content}}}\n        </div>\n    </div>\n    {{/each}}\n</div>\n{{/if}}\n<header class=\"header\" role=\"banner\">\n\n    <a href=\"#\" class=\"mobileMenu-toggle\" data-mobile-menu-toggle=\"menu\">\n        <span class=\"mobileMenu-toggleIcon\">Toggle menu</span>\n    </a>\n\n    <!-- original template -->\n    <!-- {{> components/common/navigation}} -->\n    {{> pages/custom/custom-navigation}}\n\n    {{#if page_type '===' 'default'}}\n    {{> pages/custom/custom-logo-header-head}}\n    <!-- <h1 class=\"header-logo header-logo--{{theme_settings.logo-position}}\">\n        {{> components/common/store-logo}}\n    </h1> -->\n    {{else}}\n\n    {{> pages/custom/custom-logo-header-head}}\n    <!-- <div class=\"header-logo header-logo--{{theme_settings.logo-position}}\">\n        {{> components/common/store-logo}}\n    </div> -->\n    {{/if}}\n    <div class=\"navPages-container\" id=\"menu\" data-menu>\n\n        <!-- original template -->\n        <!-- {{> components/common/navigation-menu}} -->\n\n        {{> pages/custom/custom-navigation-menu}}\n\n    </div>\n\n    {{{region name=\"header_navigation_bottom--global\"}}}\n\n</header>\n\n<!-- {{{region name=\"header_bottom--global\"}}}\n{{{region name=\"header_bottom\"}}} -->",
  "components/common/navigation": "<nav class=\"navUser\">\n    \n    {{#or customer (unless settings.hide_price_from_guests)}}\n        {{> components/common/currency-selector}}\n    {{/or}}\n\n    <ul class=\"navUser-section navUser-section--alt\">\n        {{#if customer.store_credit.value '>' 0}}\n            <li class=\"navUser-item\">\n                <a class=\"navUser-action navUser-action--storeCredit\"\n                   data-dropdown=\"storeCredit-dropdown\"\n                   data-options=\"align:bottom\"\n                   href=\"{{urls.cart}}\"\n                   aria-label=\"{{lang 'common.store_credit_overview' credit=customer.store_credit.formatted}}\"\n                >\n                    <span class=\"navUser-action-divider\">\n                        {{lang 'common.store_credit_overview' credit=customer.store_credit.formatted}}\n                    </span>\n                </a>\n                <div class=\"dropdown-menu\" id=\"storeCredit-dropdown\" data-dropdown-content aria-hidden=\"true\">\n                    {{{lang 'common.store_credit' store_credit=customer.store_credit.formatted}}}\n                </div>\n            </li>\n        {{/if}}\n        <li class=\"navUser-item\">\n            <a class=\"navUser-action navUser-item--compare\"\n               href=\"{{urls.compare}}\"\n               data-compare-nav\n               aria-label=\"{{lang 'common.compare'}}\"\n            >\n                {{lang 'common.compare'}} <span class=\"countPill countPill--positive countPill--alt\"></span>\n            </a>\n        </li>\n        {{#if theme_settings.social_icon_placement_top}}\n            <li class=\"navUser-item navUser-item--social\">\n                {{> components/common/social-links}}\n            </li>\n            <li class=\"navUser-item navUser-item--divider\">|</li>\n        {{/if}}\n        <li class=\"navUser-item\">\n            <button class=\"navUser-action navUser-action--quickSearch\"\n               type=\"button\"\n               id=\"quick-search-expand\"\n               data-search=\"quickSearch\"\n               aria-controls=\"quickSearch\"\n               aria-label=\"{{lang 'common.search'}}\"\n            >\n                {{lang 'common.search'}}\n            </button>\n        </li>\n        {{#if settings.gift_certificates_enabled}}\n            <li class=\"navUser-item\">\n                <a class=\"navUser-action\"\n                   href=\"{{urls.gift_certificate.purchase}}\"\n                   aria-label=\"{{lang 'common.gift_cert'}}\"\n                >\n                    {{lang 'common.gift_cert'}}\n                </a>\n            </li>\n        {{/if}}\n        <li class=\"navUser-item navUser-item--account\">\n            {{#if customer}}\n                <a class=\"navUser-action\"\n                   href=\"{{urls.account.index}}\"\n                   aria-label=\"{{lang 'common.account'}}\"\n                >\n                    {{lang 'common.account'}}\n                </a>\n                <a class=\"navUser-action\"\n                   href=\"{{urls.auth.logout}}\"\n                   aria-label=\"{{lang 'common.logout'}}\"\n                >\n                    {{lang 'common.logout'}}\n                </a>\n            {{else}}\n                <a class=\"navUser-action\"\n                   href=\"{{urls.auth.login}}\"\n                   aria-label=\"{{lang 'common.login'}}\"\n                >\n                    {{lang 'common.login'}}\n                </a>\n                {{#if settings.account_creation_enabled}}\n                    <span class=\"navUser-or\">{{lang 'common.or'}}</span>\n                    <a class=\"navUser-action\"\n                       href=\"{{urls.auth.create_account}}\"\n                       aria-label=\"{{lang 'common.sign_up'}}\"\n                    >\n                        {{lang 'common.sign_up'}}\n                    </a>\n                {{/if}}\n            {{/if}}\n        </li>\n        <li class=\"navUser-item navUser-item--cart\">\n            <a class=\"navUser-action\"\n               data-cart-preview\n               data-dropdown=\"cart-preview-dropdown\"\n               data-options=\"align:right\"\n               href=\"{{urls.cart}}\"\n               aria-label=\"{{lang 'cart.nav_aria_label'}}\"\n            >\n                <span class=\"navUser-item-cartLabel\">{{lang 'common.cart'}}</span>\n                <span class=\"countPill cart-quantity\"></span>\n            </a>\n\n            <div class=\"dropdown-menu\" id=\"cart-preview-dropdown\" data-dropdown-content aria-hidden=\"true\"></div>\n        </li>\n    </ul>\n\n    <div class=\"dropdown dropdown--quickSearch\" id=\"quickSearch\" aria-hidden=\"true\" data-prevent-quick-search-close>\n        {{> components/common/quick-search name='nav-quick-search'}}\n    </div>\n\n</nav>\n",
  "pages/custom/custom-navigation": "<nav class=\"custom-navUser\">\r\n    \r\n    <ul class=\"custom-navUser-section\">\r\n        {{#each categories}}\r\n            {{#if name '===' \"HOME\"}}\r\n                <li>\r\n                    <button>\r\n                        <a class=\"navPages-action{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\" aria-label=\"{{name}}\">\r\n                            <img src=\"{{cdn 'assets/img/custom-img/home_icon.png'}}\" /> {{name}}\r\n                        </a>\r\n                    </button>\r\n                </li>\r\n            {{else}}\r\n            {{/if}}\r\n        {{/each}}\r\n        {{#each pages}}\r\n            {{#if name '===' \"Blog\"}}\r\n                <li class=\"navPages-item navPages-item-page\">\r\n                    <a class=\"navPages-action{{#if name '==' ../page.title}} activePage{{/if}}\" href=\"{{url}}\"\r\n                        aria-label=\"{{name}}\">\r\n                        <img src=\"{{cdn 'assets/img/custom-img/icon_message.png'}}\" /> {{name}}\r\n                    </a>\r\n                </li>\r\n            {{/if}}\r\n        {{/each}}\r\n\r\n        <li class=\"navPages-item navPages-item-page\">\r\n            <button>\r\n                <a class=\"navPages-action{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\" aria-label=\"{{name}}\">\r\n                    <img src=\"{{cdn 'assets/img/custom-img/location_icon.png'}}\" /> Track my Order\r\n                </a>\r\n            </button>\r\n        </li>\r\n\r\n        {{#each pages}}\r\n            {{#if name '===' \"Help\"}}\r\n                <li class=\"navPages-item navPages-item-page\">\r\n                    <button>\r\n                        <a class=\"navPages-action{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\" aria-label=\"{{name}}\">\r\n                            <img src=\"{{cdn 'assets/img/custom-img/help_icon.png'}}\" /> {{name}}\r\n                        </a>\r\n                    </button>\r\n                </li>\r\n            {{/if}}\r\n        {{/each}}\r\n\r\n    </ul>\r\n\r\n    <ul class=\"custom-navUser-section\">\r\n\r\n        <li>\r\n            <a class=\"navPages-action{{#if is_active}} activePage{{/if}}\" href=\"{{urls.auth.login}}\"\r\n                aria-label=\"{{name}}\">\r\n                <img src=\"{{cdn 'assets/img/custom-img/key_icon.png'}}\" /> login\r\n            </a>\r\n        </li>\r\n\r\n        {{#if customer '===' null}}\r\n        <li>\r\n            <a class=\"navPages-action{{#if is_active}} activePage{{/if}}\" href=\"{{urls.auth.login}}\"\r\n                aria-label=\"{{name}}\">\r\n                <img src=\"{{cdn 'assets/img/custom-img/wishlist_icon.png'}}\" /> My Wishlist\r\n            </a>\r\n        </li>\r\n        {{else}}\r\n        <li>\r\n            <a class=\"navPages-action{{#if is_active}} activePage{{/if}}\" href=\"{{urls.account.wishlists.all}}\"\r\n                aria-label=\"{{name}}\">\r\n                <img src=\"{{cdn 'assets/img/custom-img/wishlist_icon.png'}}\" /> My Wishlist\r\n            </a>\r\n        </li>\r\n        {{/if}}\r\n\r\n        {{#if customer '===' null}}\r\n            <li>\r\n                <a class=\"navPages-action{{#if is_active}} activePage{{/if}}\" href=\"{{urls.auth.login}}\"\r\n                    aria-label=\"{{name}}\">\r\n                    <img src=\"{{cdn 'assets/img/custom-img/user_icon.png'}}\" /> My Account\r\n                </a>\r\n            </li>\r\n        {{else}}\r\n            <li>\r\n                <a class=\"navPages-action{{#if is_active}} activePage{{/if}}\" href=\"{{urls.account.index}}\"\r\n                    aria-label=\"{{name}}\">\r\n                    <img src=\"{{cdn 'assets/img/custom-img/user_icon.png'}}\" /> My Account\r\n                </a>\r\n            </li>\r\n        {{/if}}\r\n\r\n        {{#if customer '===' null}}\r\n        <li>\r\n            <a class=\"navPages-action{{#if is_active}} activePage{{/if}}\" href=\"{{urls.cart}}\" aria-label=\"{{name}}\">\r\n                <img src=\"{{cdn 'assets/img/custom-img/cart_icon.png'}}\" /> My Cart\r\n            </a>\r\n        </li>\r\n        {{else}}\r\n        <li>\r\n            <a class=\"navPages-action{{#if is_active}} activePage{{/if}}\" href=\"{{urls.account.orders.all}}\"\r\n                aria-label=\"{{name}}\">\r\n                <img src=\"{{cdn 'assets/img/custom-img/cart_icon.png'}}\" /> My Cart\r\n            </a>\r\n        </li>\r\n        {{/if}}\r\n\r\n    </ul>\r\n\r\n</nav>\r\n",
  "components/common/store-logo": "<a href=\"{{urls.home}}\" class=\"header-logo__link\" data-header-logo-link>\n    {{#if settings.store_logo.image}}\n        {{#if theme_settings.logo_size '===' 'original'}}\n        <img class=\"header-logo-image-unknown-size\"\n             src=\"{{getImage settings.store_logo.image 'logo_size'}}\"\n             alt=\"{{settings.store_logo.title}}\"\n             title=\"{{settings.store_logo.title}}\">\n        {{else}}\n        <div class=\"header-logo-image-container\">\n            <img class=\"header-logo-image\"\n                 src=\"{{getImage settings.store_logo.image 'logo_size'}}\"\n                 srcset=\"{{getImageSrcset1x2x settings.store_logo.image theme_settings.logo_size}}\"\n                 alt=\"{{settings.store_logo.title}}\"\n                 title=\"{{settings.store_logo.title}}\">\n        </div>\n        {{/if}}\n    {{else}}\n    <span class=\"header-logo-text\">{{settings.store_logo.title}}</span>\n    {{/if}}\n</a>\n",
  "pages/custom/custom-logo-header-head": "<h1 class=\"custom-header-logo header-logo--{{theme_settings.logo-position}}\">\r\n\r\n    <div class=\"inner-custom-header-logo\">\r\n\r\n        {{> pages/custom/custom-store-logo}}\r\n\r\n        <!-- custom search bar -->\r\n        <div class=\"custom-search-bar-container\">\r\n            <div class=\"custom-inner-search-bar-container\">\r\n                <div class=\"main-search-bar-container\">\r\n                    <div class=\"navPages-quickSearch\">\r\n                        {{> pages/custom/custom-template/custom-quick-search name='nav-menu-quick-search'}}\r\n                    </div>\r\n                    <img src=\"{{cdn 'assets/img/custom-img/search_icon.png'}}\" class=\"search-icon\" />\r\n                </div>\r\n                <img src=\"{{cdn 'assets/img/custom-img/cart_logo.png'}}\" class=\"cart-logo\" />\r\n            </div>\r\n        </div>\r\n    </div>\r\n\r\n</h1>",
  "components/common/navigation-menu": "<nav class=\"navPages\">\n    <div class=\"navPages-quickSearch\">\n        {{> pages/custom/custom-template/custom-quick-search name='nav-menu-quick-search'}}\n    </div>\n    <ul class=\"navPages-list{{#if theme_settings.navigation_design '!==' 'simple'}} navPages-list-depth-max{{/if}}\">\n        {{#each categories}}\n        <li class=\"navPages-item\">\n            {{#if ../theme_settings.navigation_design '===' 'simple'}}\n            {{> components/common/navigation-list}}\n            {{/if}}\n            {{#if ../theme_settings.navigation_design '===' 'alternate'}}\n            {{> components/common/navigation-list-alternate}}\n            {{/if}}\n        </li>\n        {{/each}}\n        {{#unless theme_settings.hide_content_navigation}}\n        {{#each pages}}\n        <li class=\"navPages-item navPages-item-page\">\n            <a class=\"navPages-action{{#if name '==' ../page.title}} activePage{{/if}}\" href=\"{{url}}\"\n                aria-label=\"{{name}}\">\n                {{name}}\n            </a>\n        </li>\n        {{/each}}\n        {{/unless}}\n    </ul>\n    <ul class=\"navPages-list navPages-list--user\">\n        {{#if currency_selector.currencies.length '>' 1}}\n        <li class=\"navPages-item\">\n            <a class=\"navPages-action has-subMenu\" href=\"#\" data-collapsible=\"navPages-currency\"\n                aria-controls=\"navPages-currency\" aria-expanded=\"false\"\n                aria-label=\"{{lang 'common.currency' code=currency_selector.active_currency_code}}\">\n                {{lang 'common.currency' code=currency_selector.active_currency_code}}\n                <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">\n                    <svg>\n                        <use href=\"#icon-chevron-down\"></use>\n                    </svg>\n                </i>\n            </a>\n            <div class=\"navPage-subMenu\" id=\"navPages-currency\" aria-hidden=\"true\" tabindex=\"-1\">\n                <ul class=\"navPage-subMenu-list\">\n                    {{#each currency_selector.currencies}}\n                    <li class=\"navPage-subMenu-item\">\n                        <a class=\"navPage-subMenu-action navPages-action\" href=\"{{{switch_url}}}\" aria-label=\"{{name}}\"\n                            data-currency-code=\"{{{code}}}\" {{#if cart_currency_switch_url.length '>' 0 }}\n                            data-cart-currency-switch-url=\"{{{cart_currency_switch_url}}}\" {{/if}}\n                            data-warning=\"{{lang 'common.currency_switch_promotion'}}\">\n                            {{#if is_active}}\n                            <strong>{{name}}</strong>\n                            {{else}}\n                            {{name}}\n                            {{/if}}\n                        </a>\n                    </li>\n                    {{/each}}\n                </ul>\n            </div>\n        </li>\n        {{/if}}\n        {{#if settings.gift_certificates_enabled}}\n        <li class=\"navPages-item\">\n            <a class=\"navPages-action\" href=\"{{urls.gift_certificate.purchase}}\"\n                aria-label=\"{{lang 'common.gift_cert'}}\">\n                {{lang 'common.gift_cert'}}\n            </a>\n        </li>\n        {{/if}}\n        {{#if customer.store_credit.value '>' 0}}\n        <li class=\"navPages-item\">\n            <a class=\"navPages-action navPages-action--storeCredit\">\n                {{lang 'common.store_credit_overview' credit=customer.store_credit.formatted}}\n            </a>\n        </li>\n        {{/if}}\n        {{#if theme_settings.product_list_display_mode '==' 'list'}}\n        <li class=\"navPages-item\">\n            <a class=\"navPages-action navPages-action--compare\" href=\"{{urls.compare}}\" data-compare-nav\n                aria-label=\"{{lang 'common.compare'}}\">\n                {{lang 'common.compare'}}\n                <span class=\"countPill countPill--positive countPill--alt\"></span>\n            </a>\n        </li>\n        {{/if}}\n        {{#if customer}}\n        <li class=\"navPages-item\">\n            <a class=\"navPages-action has-subMenu\" href=\"{{urls.account.index}}\" data-collapsible=\"navPages-account\"\n                aria-label=\"{{lang 'common.account'}}\">\n                {{lang 'common.account'}}\n                <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">\n                    <svg>\n                        <use href=\"#icon-chevron-down\" />\n                    </svg>\n                </i>\n            </a>\n            <div class=\"navPage-subMenu\" id=\"navPages-account\" aria-hidden=\"true\" tabindex=\"-1\">\n                <ul class=\"navPage-subMenu-list\">\n                    <li class=\"navPage-subMenu-item\">\n                        <a class=\"navPage-subMenu-action navPages-action\" href=\"{{urls.account.orders.all}}\"\n                            aria-label=\"{{lang 'account.nav.orders'}}\">\n                            {{lang 'account.nav.orders'}}\n                        </a>\n                    </li>\n                    {{#if settings.returns_enabled}}\n                    <li class=\"navPage-subMenu-item\">\n                        <a class=\"navPage-subMenu-action navPages-action\" href=\"{{urls.account.returns}}\"\n                            aria-label=\"{{lang 'account.nav.returns'}}\">\n                            {{lang 'account.nav.returns'}}\n                        </a>\n                    </li>\n                    {{/if}}\n                    <li class=\"navPage-subMenu-item\">\n                        <a class=\"navPage-subMenu-action navPages-action\" href=\"{{urls.account.inbox}}\"\n                            aria-label=\"{{lang 'account.mobile_nav.messages'}}\">\n                            {{lang 'account.mobile_nav.messages'}}\n                        </a>\n                    </li>\n                    <li class=\"navPage-subMenu-item\">\n                        <a class=\"navPage-subMenu-action navPages-action\" href=\"{{urls.account.addresses}}\"\n                            aria-label=\"{{lang 'account.nav.addresses'}}\">\n                            {{lang 'account.nav.addresses'}}\n                        </a>\n                    </li>\n                    {{#if settings.show_payment_methods}}\n                    <li class=\"navPage-subMenu-item\">\n                        <a class=\"navPage-subMenu-action navPages-action\" href=\"{{urls.account.payment_methods.all}}\"\n                            aria-label=\"{{lang 'account.nav.payment_methods'}}\">\n                            {{lang 'account.nav.payment_methods'}}\n                        </a>\n                    </li>\n                    {{/if}}\n                    <li class=\"navPage-subMenu-item\">\n                        <a class=\"navPage-subMenu-action navPages-action\" href=\"{{urls.account.wishlists.all}}\"\n                            aria-label=\"{{lang 'account.mobile_nav.wishlists'}}\">\n                            {{lang 'account.mobile_nav.wishlists'}}\n                        </a>\n                    </li>\n                    <li class=\"navPage-subMenu-item\">\n                        <a class=\"navPage-subMenu-action navPages-action\" href=\"{{urls.account.recent_items}}\"\n                            aria-label=\"{{lang 'account.nav.recently_viewed'}}\">\n                            {{lang 'account.nav.recently_viewed'}}\n                        </a>\n                    </li>\n                    <li class=\"navPage-subMenu-item\">\n                        <a class=\"navPage-subMenu-action navPages-action\" href=\"{{urls.account.details}}\"\n                            aria-label=\"{{lang 'account.nav.settings'}}\">\n                            {{lang 'account.nav.settings'}}\n                        </a>\n                    </li>\n                </ul>\n            </div>\n        </li>\n        <li class=\"navPages-item\">\n            <a class=\"navPages-action\" href=\"{{urls.auth.logout}}\" aria-label=\"{{lang 'common.logout'}}\">\n                {{lang 'common.logout'}}\n            </a>\n        </li>\n        {{else}}\n        <li class=\"navPages-item\">\n            <a class=\"navPages-action\" href=\"{{urls.auth.login}}\" aria-label=\"{{lang 'common.login'}}\">\n                {{lang 'common.login'}}\n            </a>\n            {{#if settings.account_creation_enabled}}\n            {{lang 'common.or'}}\n            <a class=\"navPages-action\" href=\"{{urls.auth.create_account}}\" aria-label=\"{{lang 'common.sign_up'}}\">\n                {{lang 'common.sign_up'}}\n            </a>\n            {{/if}}\n        </li>\n        {{/if}}\n        {{#if theme_settings.social_icon_placement_top}}\n        <li class=\"navPages-item\">\n            {{> components/common/social-links}}\n        </li>\n        {{/if}}\n    </ul>\n</nav>",
  "pages/custom/custom-navigation-menu": "<nav class=\"custom-navPages\">\r\n\r\n    <div class=\"navPages-quickSearch\">\r\n        {{> pages/custom/custom-template/custom-quick-search name='nav-menu-quick-search'}}\r\n    </div>\r\n\r\n    <ul class=\"navPages-list{{#if theme_settings.navigation_design '!==' 'simple'}} navPages-list-depth-max{{/if}}\">\r\n        {{#each categories}}\r\n        <li class=\"navPages-item\">\r\n            {{#if ../theme_settings.navigation_design '===' 'simple'}}\r\n            <!-- custom scss only -->\r\n            {{> pages/custom/custom-navigation-list}}\r\n\r\n            {{/if}}\r\n            {{#if ../theme_settings.navigation_design '===' 'alternate'}}\r\n            {{> components/common/navigation-list-alternate}}\r\n            {{/if}}\r\n        </li>\r\n        {{/each}}\r\n        {{#unless theme_settings.hide_content_navigation}}\r\n        {{#each pages}}\r\n        <!-- ship/contact/us -->\r\n        <!-- <li class=\"navPages-item navPages-item-page\">\r\n                     <a class=\"navPages-action{{#if name '==' ../page.title}} activePage{{/if}}\"\r\n                        href=\"{{url}}\"\r\n                        aria-label=\"{{name}}\"\r\n                     >\r\n                         {{name}}\r\n                     </a>\r\n                 </li> -->\r\n        {{/each}}\r\n        {{/unless}}\r\n    </ul>\r\n\r\n    <ul class=\"navPages-list navPages-list--user\">\r\n        {{#if customer}}\r\n        <li class=\"navPages-item\">\r\n            <a class=\"navPages-action has-subMenu\" href=\"{{urls.account.index}}\" data-collapsible=\"navPages-account\"\r\n                aria-label=\"{{lang 'common.account'}}\">\r\n                {{lang 'common.account'}}\r\n                <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">\r\n                    <svg>\r\n                        <use href=\"#icon-chevron-down\" />\r\n                    </svg>\r\n                </i>\r\n            </a>\r\n            <div class=\"navPage-subMenu\" id=\"navPages-account\" aria-hidden=\"true\" tabindex=\"-1\">\r\n                <ul class=\"navPage-subMenu-list\">\r\n                    <li class=\"navPage-subMenu-item\">\r\n                        <a class=\"navPage-subMenu-action navPages-action\" href=\"{{urls.account.orders.all}}\"\r\n                            aria-label=\"{{lang 'account.nav.orders'}}\">\r\n                            {{lang 'account.nav.orders'}}\r\n                        </a>\r\n                    </li>\r\n                    {{#if settings.returns_enabled}}\r\n                    <li class=\"navPage-subMenu-item\">\r\n                        <a class=\"navPage-subMenu-action navPages-action\" href=\"{{urls.account.returns}}\"\r\n                            aria-label=\"{{lang 'account.nav.returns'}}\">\r\n                            {{lang 'account.nav.returns'}}\r\n                        </a>\r\n                    </li>\r\n                    {{/if}}\r\n                    <li class=\"navPage-subMenu-item\">\r\n                        <a class=\"navPage-subMenu-action navPages-action\" href=\"{{urls.account.inbox}}\"\r\n                            aria-label=\"{{lang 'account.mobile_nav.messages'}}\">\r\n                            {{lang 'account.mobile_nav.messages'}}\r\n                        </a>\r\n                    </li>\r\n                    <li class=\"navPage-subMenu-item\">\r\n                        <a class=\"navPage-subMenu-action navPages-action\" href=\"{{urls.account.addresses}}\"\r\n                            aria-label=\"{{lang 'account.nav.addresses'}}\">\r\n                            {{lang 'account.nav.addresses'}}\r\n                        </a>\r\n                    </li>\r\n                    {{#if settings.show_payment_methods}}\r\n                    <li class=\"navPage-subMenu-item\">\r\n                        <a class=\"navPage-subMenu-action navPages-action\" href=\"{{urls.account.payment_methods.all}}\"\r\n                            aria-label=\"{{lang 'account.nav.payment_methods'}}\">\r\n                            {{lang 'account.nav.payment_methods'}}\r\n                        </a>\r\n                    </li>\r\n                    {{/if}}\r\n                    <li class=\"navPage-subMenu-item\">\r\n                        <a class=\"navPage-subMenu-action navPages-action\" href=\"{{urls.account.wishlists.all}}\"\r\n                            aria-label=\"{{lang 'account.mobile_nav.wishlists'}}\">\r\n                            {{lang 'account.mobile_nav.wishlists'}}\r\n                        </a>\r\n                    </li>\r\n                    <li class=\"navPage-subMenu-item\">\r\n                        <a class=\"navPage-subMenu-action navPages-action\" href=\"{{urls.account.recent_items}}\"\r\n                            aria-label=\"{{lang 'account.nav.recently_viewed'}}\">\r\n                            {{lang 'account.nav.recently_viewed'}}\r\n                        </a>\r\n                    </li>\r\n                    <li class=\"navPage-subMenu-item\">\r\n                        <a class=\"navPage-subMenu-action navPages-action\" href=\"{{urls.account.details}}\"\r\n                            aria-label=\"{{lang 'account.nav.settings'}}\">\r\n                            {{lang 'account.nav.settings'}}\r\n                        </a>\r\n                    </li>\r\n                </ul>\r\n            </div>\r\n        </li>\r\n        <li class=\"navPages-item\">\r\n            <a class=\"navPages-action\" href=\"{{urls.auth.logout}}\" aria-label=\"{{lang 'common.logout'}}\">\r\n                {{lang 'common.logout'}}\r\n            </a>\r\n        </li>\r\n        {{else}}\r\n        <li class=\"navPages-item\">\r\n            <a class=\"navPages-action\" href=\"{{urls.auth.login}}\" aria-label=\"{{lang 'common.login'}}\">\r\n                {{lang 'common.login'}}\r\n            </a>\r\n            {{#if settings.account_creation_enabled}}\r\n            <a class=\"navPages-action\" href=\"{{urls.auth.create_account}}\" aria-label=\"{{lang 'common.sign_up'}}\">\r\n                {{lang 'common.sign_up'}}\r\n            </a>\r\n            {{/if}}\r\n        </li>\r\n        {{/if}}\r\n    </ul>\r\n\r\n</nav>",
  "components/common/currency-selector": "{{#if currency_selector.currencies.length '>' 1}}\n<ul class=\"navUser-section\">\n    <li class=\"navUser-item\">\n        <a class=\"navUser-action navUser-action--currencySelector has-dropdown\"\n           href=\"#\"\n           data-dropdown=\"currencySelection\"\n           aria-controls=\"currencySelection\"\n           aria-expanded=\"false\"\n        >\n            {{lang 'common.currency' code=currency_selector.active_currency_code}}\n            <i class=\"icon\" aria-hidden=\"true\">\n                <svg>\n                    <use href=\"#icon-chevron-down\" />\n                </svg>\n            </i>\n        </a>\n        <ul class=\"dropdown-menu\" id=\"currencySelection\" data-dropdown-content aria-hidden=\"true\" tabindex=\"-1\">\n            {{#each currency_selector.currencies}}\n            <li class=\"dropdown-menu-item\">\n                <a href=\"{{{switch_url}}}\"\n                   data-currency-code=\"{{{code}}}\"\n                   {{#if cart_currency_switch_url.length '>' 0 }}\n                   data-cart-currency-switch-url=\"{{{cart_currency_switch_url}}}\"\n                   {{/if}}\n                   data-warning=\"{{lang 'common.currency_switch_promotion'}}\"\n                >\n                {{#if is_active}}\n                    <strong>{{name}}</strong>\n                {{else}}\n                    {{name}}\n                {{/if}}\n                </a>\n            </li>\n            {{/each}}\n        </ul>\n    </li>\n</ul>\n{{/if}}\n",
  "components/common/social-links": "{{#if social_media}}\n    <ul class=\"socialLinks socialLinks--alt\">\n        {{#each social_media}}\n            {{#if display_name '===' 'twitter'}}\n                <li class=\"socialLinks-item\">\n                    <a class=\"icon icon--x\"\n                    href=\"{{url}}\"\n                    target=\"_blank\"\n                    rel=\"noopener\"\n                    title=\"X\"\n                    >\n                        <span class=\"aria-description--hidden\">X</span>\n                        <svg>\n                            <use href=\"#icon-x\"/>\n                        </svg>\n                    </a>\n                </li>\n            {{else}}\n                <li class=\"socialLinks-item\">\n                    <a class=\"icon icon--{{display_name}}\"\n                    href=\"{{url}}\"\n                    target=\"_blank\"\n                    rel=\"noopener\"\n                    title=\"{{{capitalize display_name}}}\"\n                    >\n                        <span class=\"aria-description--hidden\">{{{capitalize display_name}}}</span>\n                        <svg>\n                            <use href=\"#icon-{{display_name}}\"/>\n                        </svg>\n                    </a>\n                </li>\n            {{/if}}\n        {{/each}}\n    </ul>\n{{/if}}\n",
  "components/common/quick-search": "<div class=\"container\">\n    <form class=\"form\" onsubmit=\"return false\" data-url=\"{{urls.search}}\" data-quick-search-form>\n        <fieldset class=\"form-fieldset\">\n            <div class=\"form-field\">\n                <label class=\"is-srOnly\" for=\"{{name}}\">{{lang \"search.quick_search.input_label\"}}</label>\n                <input class=\"form-input\"\n                       data-search-quick\n                       name=\"{{name}}\"\n                       id=\"{{name}}\"\n                       data-error-message=\"{{lang 'search.error.empty_field'}}\"\n                       placeholder=\"{{lang 'search.quick_search.input_placeholder'}}\"\n                       autocomplete=\"off\"\n                >\n            </div>\n        </fieldset>\n    </form>\n    <section class=\"quickSearchResults\" data-bind=\"html: results\"></section>\n    <p role=\"status\"\n       aria-live=\"polite\"\n       class=\"aria-description--hidden\"\n       data-search-aria-message-predefined-text=\"{{{lang 'search.results.quick_count_live'}}}\"\n    ></p>\n</div>\n",
  "pages/custom/custom-template/custom-quick-search": "<div class=\"container\">\r\n    <form class=\"form\" onsubmit=\"return false\" data-url=\"{{urls.search}}\" data-quick-search-form>\r\n        <fieldset class=\"form-fieldset\">\r\n            <div class=\"form-field\">\r\n                <input class=\"form-input\" name=\"{{name}}\" id=\"{{name}}\"\r\n                    data-error-message=\"{{lang 'search.error.empty_field'}}\"\r\n                    placeholder=\"{{lang 'search.quick_search.input_placeholder'}}\" autocomplete=\"off\">\r\n            </div>\r\n        </fieldset>\r\n    </form>\r\n</div>",
  "pages/custom/custom-store-logo": "<a href=\"{{urls.home}}\" class=\"header-logo__link\" data-header-logo-link>\r\n    {{#if settings.store_logo.image}}\r\n    {{#if theme_settings.logo_size '===' 'original'}}\r\n    <img class=\"header-logo-image-unknown-size\" src=\"{{getImage settings.store_logo.image 'logo_size'}}\"\r\n        alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\r\n    {{else}}\r\n    <div class=\"header-logo-image-container\">\r\n        <img class=\"header-logo-image\" src=\"{{getImage settings.store_logo.image 'logo_size'}}\"\r\n            srcset=\"{{getImageSrcset1x2x settings.store_logo.image theme_settings.logo_size}}\"\r\n            alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\r\n    </div>\r\n    {{/if}}\r\n    {{else}}\r\n    <span class=\"header-logo-text\">{{settings.store_logo.title}}</span>\r\n    {{/if}}\r\n</a>\n",
  "components/common/navigation-list": "{{#if children}}\n<a class=\"navPages-action has-subMenu{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\"\n    data-collapsible=\"navPages-{{id}}\">\n    {{name}}\n    <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">\n        <svg>\n            <use href=\"#icon-chevron-down\" />\n        </svg>\n    </i>\n</a>\n<div class=\"navPage-subMenu\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\n    <ul class=\"navPage-subMenu-list\">\n        <li class=\"navPage-subMenu-item\">\n            <a class=\"navPage-subMenu-action navPages-action\" href=\"{{url}}\"\n                aria-label=\"{{lang 'category.view_all.name' category=name}}\">\n                {{lang 'category.view_all.name' category=name}}\n            </a>\n        </li>\n        {{#each children}}\n        <li class=\"navPage-subMenu-item\">\n            {{#if children}}\n            <a class=\"navPage-subMenu-action navPages-action has-subMenu{{#if is_active}} activePage{{/if}}\"\n                href=\"{{url}}\" aria-label=\"{{name}}\">\n                {{name}}\n                <span class=\"collapsible-icon-wrapper\" data-collapsible=\"navPages-{{id}}\"\n                    data-collapsible-disabled-breakpoint=\"medium\" data-collapsible-disabled-state=\"open\"\n                    data-collapsible-enabled-state=\"closed\">\n                    <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">\n                        <svg>\n                            <use href=\"#icon-chevron-down\" />\n                        </svg>\n                    </i>\n                </span>\n            </a>\n            <ul class=\"navPage-childList\" id=\"navPages-{{id}}\">\n                {{#each children}}\n                <li class=\"navPage-childList-item\">\n                    <a class=\"navPage-childList-action navPages-action{{#if is_active}} activePage{{/if}}\"\n                        href=\"{{url}}\" aria-label=\"{{name}}\">\n                        {{name}}\n                    </a>\n                </li>\n                {{/each}}\n            </ul>\n            {{else}}\n            <a class=\"navPage-subMenu-action navPages-action{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\"\n                aria-label=\"{{name}}\">\n                {{name}}\n            </a>\n            {{/if}}\n        </li>\n        {{/each}}\n    </ul>\n</div>\n{{else}}\n<a class=\"navPages-action{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\" aria-label=\"{{name}}\">\n    {{name}}\n</a>\n{{/if}}\n",
  "components/common/navigation-list-alternate": "{{#if children}}\n    <a class=\"navPages-action navPages-action-depth-max has-subMenu is-root{{#if is_active}} activePage{{/if}}\"\n       href=\"{{url}}\"\n       data-collapsible=\"navPages-{{id}}\"\n       aria-label=\"{{name}}\"\n    >\n        {{name}}\n        <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">\n            <svg><use href=\"#icon-chevron-down\" /></svg>\n        </i>\n    </a>\n    {{> components/common/navigation-dropdown}}\n{{else}}\n    <a class=\"navPages-action{{#if is_active}} activePage{{/if}}\"\n       href=\"{{url}}\"\n       aria-label=\"{{name}}\"\n    >\n        {{name}}\n    </a>\n{{/if}}\n",
  "pages/custom/custom-navigation-list": "{{#if children}}\r\n    <a class=\"navPages-action has-subMenu{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\"\r\n        data-collapsible=\"navPages-{{id}}\">\r\n        {{name}}\r\n        <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">\r\n            <svg>\r\n                <use href=\"#icon-chevron-down\" />\r\n            </svg>\r\n        </i>\r\n    </a>\r\n    <div class=\"navPage-subMenu\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\r\n        <ul class=\"navPage-subMenu-list\">\r\n            <li class=\"navPage-subMenu-item\">\r\n                <a class=\"navPage-subMenu-action navPages-action\" href=\"{{url}}\"\r\n                    aria-label=\"{{lang 'category.view_all.name' category=name}}\">\r\n                    {{lang 'category.view_all.name' category=name}}\r\n                </a>\r\n            </li>\r\n            {{#each children}}\r\n            <li class=\"navPage-subMenu-item\">\r\n                {{#if children}}\r\n                <a class=\"navPage-subMenu-action navPages-action has-subMenu{{#if is_active}} activePage{{/if}}\"\r\n                    href=\"{{url}}\" aria-label=\"{{name}}\">\r\n                    {{name}}\r\n                    <span class=\"collapsible-icon-wrapper\" data-collapsible=\"navPages-{{id}}\"\r\n                        data-collapsible-disabled-breakpoint=\"medium\" data-collapsible-disabled-state=\"open\"\r\n                        data-collapsible-enabled-state=\"closed\">\r\n                        <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">\r\n                            <svg>\r\n                                <use href=\"#icon-chevron-down\" />\r\n                            </svg>\r\n                        </i>\r\n                    </span>\r\n                </a>\r\n                <ul class=\"navPage-childList\" id=\"navPages-{{id}}\">\r\n                    {{#each children}}\r\n                    <li class=\"navPage-childList-item\">\r\n                        <a class=\"navPage-childList-action navPages-action{{#if is_active}} activePage{{/if}}\"\r\n                            href=\"{{url}}\" aria-label=\"{{name}}\">\r\n                            {{name}}\r\n                        </a>\r\n                    </li>\r\n                    {{/each}}\r\n                </ul>\r\n                {{else}}\r\n                <a class=\"navPage-subMenu-action navPages-action{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\"\r\n                    aria-label=\"{{name}}\">\r\n                    {{name}}\r\n                </a>\r\n                {{/if}}\r\n            </li>\r\n            {{/each}}\r\n        </ul>\r\n    </div>\r\n{{else}}\r\n    <!-- Home -->\r\n    <a class=\"navPages-action{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\" aria-label=\"{{name}}\">\r\n        {{name}}\r\n    </a>\r\n{{/if}}\n",
  "components/common/navigation-dropdown": "<div class=\"navPage-subMenu navPage-subMenu-horizontal\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\n    <ul class=\"navPage-subMenu-list\">\n        <li class=\"navPage-subMenu-item-parent\">\n            <a class=\"navPage-subMenu-action navPages-action navPages-action-depth-max\" href=\"{{url}}\">{{lang 'category.view_all.name' category=name}}</a>\n        </li>\n        {{#each children}}\n            <li class=\"navPage-subMenu-item-child\">\n                {{#if children}}\n                    <a class=\"navPage-subMenu-action navPages-action navPages-action-depth-max has-subMenu{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\" data-collapsible=\"navPages-{{id}}\">\n                        {{name}}<i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use href=\"#icon-chevron-down\" /></svg></i>\n                    </a>\n                    {{> components/common/navigation-dropdown}}\n                {{else}}\n                    <a class=\"navPage-subMenu-action navPages-action{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\">{{name}}</a>\n                {{/if}}\n            </li>\n        {{/each}}\n    </ul>\n</div>\n"
}
