{
  "pages/custom/custom-footer-inner-container": "<div class=\"container\" style=\"margin: 0px;\">\r\n\r\n    <section class=\"custom-footer-info\">\r\n\r\n        <article class=\"shop-by footer-info-col footer-info-col--small\" data-section-type=\"footer-categories\">\r\n            <h3 class=\"footer-info-heading\">{{lang 'SHOP BY'}}</h3>\r\n            <ul class=\"footer-info-list\">\r\n                {{#each theme_settings.shop_by_url}}\r\n                {{#if name '===' \"HOME\"}}\r\n                {{else}}\r\n                <li>\r\n                    <a href=\"{{url}}\">{{name}}</a>\r\n                </li>\r\n                {{/if}}\r\n                {{/each}}\r\n            </ul>\r\n        </article>\r\n\r\n        <article class=\"information footer-info-col footer-info-col--small\" data-section-type=\"footer-webPages\">\r\n            <h3 class=\"footer-info-heading\">{{lang 'INFORMATION & SUPPORT'}}</h3>\r\n            <ul class=\"footer-info-list\">\r\n                {{#each pages}}\r\n                {{#if name '===' \"Delivery Detail\"}}\r\n                <li>\r\n                    <a href=\"{{url}}\">{{name}}</a>\r\n                </li>\r\n                {{/if}}\r\n                {{#if name '===' \"Returns Policy\"}}\r\n                <li>\r\n                    <a href=\"{{url}}\">{{name}}</a>\r\n                </li>\r\n                {{/if}}\r\n                {{#if name '===' \"About Us\"}}\r\n                <li>\r\n                    <a href=\"{{url}}\">{{name}}</a>\r\n                </li>\r\n                {{/if}}\r\n                {{#if name '===' \"Terms & Conditions\"}}\r\n                <li>\r\n                    <a href=\"{{url}}\">{{name}}</a>\r\n                </li>\r\n                {{/if}}\r\n                {{#if name '===' \"My Account\"}}\r\n                <li>\r\n                    <a href=\"{{url}}\">{{name}}</a>\r\n                </li>\r\n                {{/if}}\r\n                {{#if name '===' \"FAQ\"}}\r\n                <li>\r\n                    <a href=\"{{url}}\">{{name}}</a>\r\n                </li>\r\n                {{/if}}\r\n                {{#if name '===' \"Privacy Policy\"}}\r\n                <li>\r\n                    <a href=\"{{url}}\">{{name}}</a>\r\n                </li>\r\n                {{/if}}\r\n                {{/each}}\r\n                <!-- <li>\r\n                    <a href=\"{{urls.sitemap}}\">{{lang 'sitemap.page_title'}}</a>\r\n                </li> -->\r\n            </ul>\r\n        </article>\r\n\r\n        <article class=\"services footer-info-col footer-info-col--small\" data-section-type=\"footer-webPages\">\r\n            <h3 class=\"footer-info-heading\">{{lang 'CUSTOMER SERVICES'}}</h3>\r\n            <ul class=\"footer-info-list\">\r\n                {{#each pages}}\r\n                {{#if name '===' \"About Us\"}}\r\n                <li>\r\n                    <a href=\"{{url}}\">{{name}}</a>\r\n                </li>\r\n                {{/if}}\r\n                {{#if name '===' \"Contact Us\"}}\r\n                <li>\r\n                    <a href=\"{{url}}\">{{name}}</a>\r\n                </li>\r\n                {{/if}}\r\n                {{#if name '===' \"Delivery\"}}\r\n                <li>\r\n                    <a href=\"{{url}}\">{{name}}</a>\r\n                </li>\r\n                {{/if}}\r\n                {{#if name '===' \"Sign Up for Emails\"}}\r\n                <li>\r\n                    <a href=\"{{url}}\">{{name}}</a>\r\n                </li>\r\n                {{/if}}\r\n                {{#if name '===' \"Deal Match\"}}\r\n                <li>\r\n                    <a href=\"{{url}}\">{{name}}</a>\r\n                </li>\r\n                {{/if}}\r\n                {{#if name '===' \"Financing\"}}\r\n                <li>\r\n                    <a href=\"{{url}}\">{{name}}</a>\r\n                </li>\r\n                {{/if}}\r\n                {{#if name '===' \"FAQs\"}}\r\n                <li>\r\n                    <a href=\"{{url}}\">{{name}}</a>\r\n                </li>\r\n                {{/if}}\r\n                {{#if name '===' \"Orders and Returns\"}}\r\n                <li>\r\n                    <a href=\"{{url}}\">{{name}}</a>\r\n                </li>\r\n                {{/if}}\r\n                {{/each}}\r\n                <!-- <li>\r\n                    <a href=\"{{urls.sitemap}}\">{{lang 'sitemap.page_title'}}</a>\r\n                </li> -->\r\n            </ul>\r\n        </article>\r\n\r\n    </section>\r\n\r\n</div>"
