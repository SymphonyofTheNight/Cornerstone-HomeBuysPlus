{
  "pages/custom/custom-template/subscription-container": "<div class=\"roof-icon-container\">\r\n    <img src=\"{{cdn 'assets/img/custom-img/footer_roof_img.png'}}\" class=\"img\" />\r\n</div>\r\n\r\n<article class=\"subscription-container footer-info-col\" data-section-type=\"newsletterSubscription\">\r\n    {{#if settings.show_newsletter_box}}\r\n        {{> pages/custom/custom-template/custom-subscription-form}}\r\n    {{/if}}\r\n</article>\r\n\r\n<div class=\"icon-container\">\r\n    <div class=\"inner-container\">\r\n        {{#each pages}}\r\n        {{#if name '===' \"Facebook \"}}\r\n        <a href=\"{{url}}\" aria-label=\"{{name}}\" target=\"_blank\">\r\n            <img src=\"{{cdn 'assets/img/custom-img/facebook-logo.png'}}\" />\r\n        </a>\r\n        {{/if}}\r\n        {{#if name '===' \"Twitter\"}}\r\n        <a href=\"{{url}}\" aria-label=\"{{name}}\" target=\"_blank\">\r\n            <img src=\"{{cdn 'assets/img/custom-img/tweeter-logo.png'}}\" />\r\n        </a>\r\n        {{/if}}\r\n        {{#if name '===' \"Youtube\"}}\r\n        <a href=\"{{url}}\" aria-label=\"{{name}}\" target=\"_blank\">\r\n            <img src=\"{{cdn 'assets/img/custom-img/youtube-logo.png'}}\" />\r\n        </a>\r\n        {{/if}}\r\n        {{/each}}\r\n    </div>\r\n</div>\r\n\r\n{{#if theme_settings.show_copyright_footer}}\r\n<div class=\"footer-copyright\">\r\n    <p class=\"powered-by\">&copy; {{moment format=\"YYYY\"}} {{settings.store_name}}. All rights reserved.</p>\r\n</div>\r\n{{/if}}",
  "pages/custom/custom-template/custom-subscription-form": "\r\n<h3 class=\"footer-info-heading\">  {{lang 'SIGN UP FOR OUR NEWSLETTER'}}</h3>\r\n\r\n<!-- <p>{{lang 'newsletter.subscribe_intro'}}</p> -->\r\n\r\n<form class=\"form\" action=\"{{urls.subscribe.action}}\" method=\"post\">\r\n    <fieldset class=\"form-fieldset\">\r\n        <input type=\"hidden\" name=\"action\" value=\"subscribe\">\r\n        <input type=\"hidden\" name=\"nl_first_name\" value=\"bc\">\r\n        <input type=\"hidden\" name=\"check\" value=\"1\">\r\n        <div class=\"form-field\">\r\n            <label class=\"form-label is-srOnly\" for=\"nl_email\">{{lang 'common.email_address'}}</label>\r\n            <div class=\"form-prefixPostfix wrap\">\r\n                <input class=\"form-input\"\r\n                       id=\"nl_email\"\r\n                       name=\"nl_email\"\r\n                       type=\"email\"\r\n                       value=\"{{customer.email}}\"\r\n                       placeholder=\"{{lang 'newsletter.email_placeholder'}}\"\r\n                       aria-describedby=\"alertBox-message-text\"\r\n                       aria-required=\"true\"\r\n                       required\r\n                >\r\n                <input class=\"button button--primary form-prefixPostfix-button--postfix\"\r\n                       type=\"submit\"\r\n                       value=\"{{lang 'newsletter.subscribe_submit'}}\"\r\n                >\r\n            </div>\r\n            {{#if settings.show_newsletter_summary }}\r\n                <div class=\"footer-newsletter-summary\">{{settings.newsletter_summary}}</div>\r\n            {{/if}}\r\n        </div>\r\n    </fieldset>\r\n</form>\r\n"
}
