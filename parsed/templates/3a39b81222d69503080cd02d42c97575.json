{
  "pages/custom/custom-footer-inner-container-desktop": "<div class=\"custom-footer-inner-container-desktop\">\r\n\r\n    {{#if theme_settings.social_icon_placement_bottom '!==' 'bottom_none'}}\r\n    <article\r\n        class=\"footer-info-col footer-info-col--social footer-info-col--{{#if theme_settings.social_icon_placement_bottom '===' 'bottom_left'}}left{{else}}right{{/if}}\"\r\n        data-section-type=\"footer-webPages\">\r\n        <h3 class=\"footer-info-heading\">{{lang 'social.connect'}}</h3>\r\n        {{> components/common/social-links}}\r\n    </article>\r\n    {{/if}}\r\n\r\n    <section class=\"custom-footer-info\">\r\n\r\n        <div class=\"content-container\">\r\n            <div class=\"homebuysplus\">\r\n                <img src=\"{{cdn 'assets/img/custom-img/logo.png'}}\" />\r\n                <p class=\"text-content\">\r\n                    At Home Buys Plus, we're not just about selling Furniture! Sure, as the Florida's number 1 Furniture\r\n                    store we do have one of the most exclusive collections of Furniture styles available online, but\r\n                    with our wide selection of products from amttresses to Fire Places, we cater to your entire\r\n                    furniture needs - and all at great value bargain prices!\r\n                </p>\r\n            </div>\r\n        </div>\r\n\r\n        <div class=\"content-container\">\r\n            <article class=\"shop-by footer-info-col footer-info-col--small\" data-section-type=\"footer-categories\">\r\n                <h3 class=\"footer-info-heading\">{{lang 'SHOP BY'}}</h3>\r\n                <ul class=\"footer-info-list\">\r\n                    {{#each theme_settings.shop_by_url}}\r\n                    {{#if name '===' \"HOME\"}}\r\n                    {{else}}\r\n                    <li>\r\n                        <a href=\"{{url}}\">{{name}}</a>\r\n                    </li>\r\n                    {{/if}}\r\n                    {{/each}}\r\n                </ul>\r\n            </article>\r\n        </div>\r\n\r\n        <div class=\"content-container\">\r\n            <article class=\"information footer-info-col footer-info-col--small\" data-section-type=\"footer-webPages\">\r\n                <h3 class=\"footer-info-heading\">{{lang 'INFORMATION & SUPPORT'}}</h3>\r\n                <ul class=\"footer-info-list\">\r\n                    {{#each pages}}\r\n                    {{#if name '===' \"Delivery Detail\"}}\r\n                    <li>\r\n                        <a href=\"{{url}}\">{{name}}</a>\r\n                    </li>\r\n                    {{/if}}\r\n                    {{#if name '===' \"Returns Policy\"}}\r\n                    <li>\r\n                        <a href=\"{{url}}\">{{name}}</a>\r\n                    </li>\r\n                    {{/if}}\r\n                    {{#if name '===' \"About Us\"}}\r\n                    <li>\r\n                        <a href=\"{{url}}\">{{name}}</a>\r\n                    </li>\r\n                    {{/if}}\r\n                    {{#if name '===' \"Terms & Conditions\"}}\r\n                    <li>\r\n                        <a href=\"{{url}}\">{{name}}</a>\r\n                    </li>\r\n                    {{/if}}\r\n                    {{#if name '===' \"My Account\"}}\r\n                    <li>\r\n                        <a href=\"{{url}}\">{{name}}</a>\r\n                    </li>\r\n                    {{/if}}\r\n                    {{#if name '===' \"FAQ\"}}\r\n                    <li>\r\n                        <a href=\"{{url}}\">{{name}}</a>\r\n                    </li>\r\n                    {{/if}}\r\n                    {{#if name '===' \"Privacy Policy\"}}\r\n                    <li>\r\n                        <a href=\"{{url}}\">{{name}}</a>\r\n                    </li>\r\n                    {{/if}}\r\n                    {{/each}}\r\n                    <!-- <li>\r\n                        <a href=\"{{urls.sitemap}}\">{{lang 'sitemap.page_title'}}</a>\r\n                    </li> -->\r\n                </ul>\r\n            </article>\r\n        </div>\r\n\r\n        <div class=\"content-container\">\r\n            <article class=\"services footer-info-col footer-info-col--small\" data-section-type=\"footer-webPages\">\r\n                <h3 class=\"footer-info-heading\">{{lang 'CUSTOMER SERVICES'}}</h3>\r\n                <ul class=\"footer-info-list\">\r\n                    {{#each pages}}\r\n                    {{#if name '===' \"About Us\"}}\r\n                    <li>\r\n                        <a href=\"{{url}}\">{{name}}</a>\r\n                    </li>\r\n                    {{/if}}\r\n                    {{#if name '===' \"Contact Us\"}}\r\n                    <li>\r\n                        <a href=\"{{url}}\">{{name}}</a>\r\n                    </li>\r\n                    {{/if}}\r\n                    {{#if name '===' \"Delivery\"}}\r\n                    <li>\r\n                        <a href=\"{{url}}\">{{name}}</a>\r\n                    </li>\r\n                    {{/if}}\r\n                    {{#if name '===' \"Sign Up for Emails\"}}\r\n                    <li>\r\n                        <a href=\"{{url}}\">{{name}}</a>\r\n                    </li>\r\n                    {{/if}}\r\n                    {{#if name '===' \"Deal Match\"}}\r\n                    <li>\r\n                        <a href=\"{{url}}\">{{name}}</a>\r\n                    </li>\r\n                    {{/if}}\r\n                    {{#if name '===' \"Financing\"}}\r\n                    <li>\r\n                        <a href=\"{{url}}\">{{name}}</a>\r\n                    </li>\r\n                    {{/if}}\r\n                    {{#if name '===' \"FAQs\"}}\r\n                    <li>\r\n                        <a href=\"{{url}}\">{{name}}</a>\r\n                    </li>\r\n                    {{/if}}\r\n                    {{#if name '===' \"Orders and Returns\"}}\r\n                    <li>\r\n                        <a href=\"{{url}}\">{{name}}</a>\r\n                    </li>\r\n                    {{/if}}\r\n                    {{/each}}\r\n                    <!-- <li>\r\n                        <a href=\"{{urls.sitemap}}\">{{lang 'sitemap.page_title'}}</a>\r\n                    </li> -->\r\n                </ul>\r\n            </article>\r\n        </div>\r\n\r\n    </section>\r\n\r\n</div>",
  "components/common/social-links": "{{#if social_media}}\n    <ul class=\"socialLinks socialLinks--alt\">\n        {{#each social_media}}\n            {{#if display_name '===' 'twitter'}}\n                <li class=\"socialLinks-item\">\n                    <a class=\"icon icon--x\"\n                    href=\"{{url}}\"\n                    target=\"_blank\"\n                    rel=\"noopener\"\n                    title=\"X\"\n                    >\n                        <span class=\"aria-description--hidden\">X</span>\n                        <svg>\n                            <use href=\"#icon-x\"/>\n                        </svg>\n                    </a>\n                </li>\n            {{else}}\n                <li class=\"socialLinks-item\">\n                    <a class=\"icon icon--{{display_name}}\"\n                    href=\"{{url}}\"\n                    target=\"_blank\"\n                    rel=\"noopener\"\n                    title=\"{{{capitalize display_name}}}\"\n                    >\n                        <span class=\"aria-description--hidden\">{{{capitalize display_name}}}</span>\n                        <svg>\n                            <use href=\"#icon-{{display_name}}\"/>\n                        </svg>\n                    </a>\n                </li>\n            {{/if}}\n        {{/each}}\n    </ul>\n{{/if}}\n"
}
