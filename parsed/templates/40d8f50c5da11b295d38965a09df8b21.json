{
  "components/common/body": "<main class=\"body\" id=\"main-content\" role=\"main\" data-currency-code=\"{{currency_selector.active_currency_code}}\"\n    style=\"margin-top: 0px !important; margin-bottom: 0px;\">\n    {{#block \"hero\"}} {{/block}}\n    <div class=\"container\">\n        {{#block \"page\"}} {{/block}}\n    </div>\n\n    <!-- custom templates show when page type to default -->\n    {{#if page_type '===' \"default\"}}\n    {{> pages/custom/custom-parallax-banner}}\n    {{> pages/custom/custom-article-label}}\n    {{> pages/custom/custom-article-container}}\n    {{/if}}\n\n    {{> components/common/modal/modal}}\n    {{> components/common/alert/alert-modal}}\n</main>",
  "pages/custom/custom-parallax-banner": "<div class=\"parallax-banner\">\r\n    {{> pages/custom/custom-template/parallax-banner}}\r\n</div>\n",
  "pages/custom/custom-article-container": "<div class=\"custom-article-container\">\r\n    {{#each theme_settings.blogs}}\r\n        {{> pages/custom/custom-template/article-card title=this.title url=this.url img=this.img-url meta=this.meta }}\r\n    {{/each}}\r\n    <div class=\"action-view-article\">\r\n        {{#each pages}}\r\n            {{#if name '===' \"Blog\"}}\r\n                <li class=\"navPages-item navPages-item-page\" style=\"list-style-type: none;\">\r\n                    <a class=\"navPages-action{{#if name '==' ../page.title}} activePage{{/if}}\" href=\"{{url}}\"\r\n                        aria-label=\"{{name}}\">\r\n                        <button class=\"btn-action\">\r\n                            View All Articles\r\n                        </button>\r\n                    </a>\r\n                </li>\r\n            {{/if}}\r\n        {{/each}}\r\n    </div>\r\n</div>",
  "pages/custom/custom-article-label": "<div class=\"custom-article-label\">\r\n    <div class=\"inner-container\">\r\n        <div class=\"content\">\r\n            <h1 class=\"label\">\r\n                RECENT TIPS / ARTICLES\r\n            </h1>\r\n            <div class=\"line-container\">\r\n                <div class=\"line\"></div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</div>\n",
  "components/common/alert/alert-modal": "<div id=\"alert-modal\" class=\"modal modal--alert modal--small\" data-reveal data-prevent-quick-search-close>\n    <div class=\"alert-icon error-icon\">\n        <span class=\"icon-content\">\n            <span class=\"line line-left\"></span>\n            <span class=\"line line-right\"></span>\n        </span>\n    </div>\n\n    <div class=\"alert-icon warning-icon\">\n        <div class=\"icon-content\">!</div>\n    </div>\n\n    <div class=\"modal-content\"></div>\n\n    <div class=\"button-container\">\n        <button type=\"button\" class=\"confirm button\" data-reveal-close>{{lang 'common.ok'}}</button>\n        <button type=\"button\" class=\"cancel button\" data-reveal-close>{{lang 'common.cancel'}}</button>\n    </div>\n</div>\n",
  "components/common/modal/modal": "<div id=\"modal\" class=\"modal\" data-reveal data-prevent-quick-search-close>\n    {{> components/common/modal/modal-close-btn}}\n    <div id=\"modal-content\" class=\"modal-content\"></div>\n    <div class=\"loadingOverlay\"></div>\n</div>\n",
  "pages/custom/custom-template/parallax-banner": "<div class=\"main-banner\">\r\n    <div class=\"outer-banner\">\r\n        <div class=\"inner-banner\">\r\n            <div class=\"content\">\r\n                <img src=\"{{cdn 'assets/img/custom-img/home-decors-text.png'}}\" class=\"banner-text-1\" />\r\n                <p class=\"p-text\">\r\n                    Lorem ipsum dolor sit amet,consectetur adipiscing elit. Nam congue turpis eu hendrerit Iobortis.\r\n                    Pellentesque eu tellus mattis, volutpat odio et, ullamcorper nulla. Etiam sir amet commodo massa.\r\n                    Maecenas\r\n                </p>\r\n                <button class=\"btn-action\">\r\n                    SHOP NOW\r\n                </button>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</div>",
  "pages/custom/custom-template/article-card": "<div class=\"article-card\">\r\n    <div class=\"title-container\">\r\n        {{title}}\r\n    </div>\r\n    <div class=\"background-img\" style=\"background-image: url('{{img}}');\"></div>\r\n    <div class=\"summary-meta\">\r\n        {{meta}} &hellip; <a href=\"{{url}}\" class=\"link\">Read More</a>\r\n    </div>\r\n</div>",
  "components/common/modal/modal-close-btn": "<button class=\"modal-close\"\n        type=\"button\"\n        title=\"{{lang 'common.close'}}\"\n        {{additional_attr}}\n>\n    <span class=\"aria-description--hidden\">{{lang 'common.close'}}</span>\n    <span aria-hidden=\"true\">&#215;</span>\n</button>\n"
}
